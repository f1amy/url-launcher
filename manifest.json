{
    "name": "URL Launcher",
    "description": "Open all of your favorite URLs in one click!",
    "version": "1.0",
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "hello_extensions.png"
    },
    "manifest_version": 2,
    "permissions": ["activeTab", "storage"]
}
